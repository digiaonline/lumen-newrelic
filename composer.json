{
    "name": "nordsoftware/lumen-newrelic",
    "description": "New Relic instrumentation for the Lumen framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Stenvall",
            "email": "sam.stenvall@nordsoftware.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "intouch/newrelic": "^1.0",
        "illuminate/support": "^5.5 | ^6.0 | ^7.0 | ^8.0",
        "nordsoftware/lumen-chained-exception-handler": "^2.0"
    },
    "require-dev": {
        "laravel/lumen-framework": "^5.5|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Nord\\Lumen\\NewRelic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nord\\Lumen\\NewRelic\\Tests\\": "tests/"
        }
    }
}
